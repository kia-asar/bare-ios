// Dev.xcconfig
// Development environment configuration

// Bundle Identifier
PRODUCT_BUNDLE_IDENTIFIER = co.bareapp.bare.dev

// Swift Compilation Conditions (includes DEBUG for debug builds)
SWIFT_ACTIVE_COMPILATION_CONDITIONS = DEBUG DEV

// PostHog Configuration
// DO NOT COMMIT: Add your actual PostHog API key here
POSTHOG_API_KEY = YOUR_POSTHOG_DEV_API_KEY_HERE
POSTHOG_HOST = https:/$()/us.i.posthog.com

// Display Name (shows in springboard)
PRODUCT_NAME = bare Dev

// App Icon
ASSETCATALOG_COMPILER_APPICON_NAME = AppIcon-Dev

// Optimization Level (inherited from base configuration)
// Dev typically uses -Onone for debugging
SWIFT_OPTIMIZATION_LEVEL = -Onone
GCC_OPTIMIZATION_LEVEL = 0

// Firebase Analytics IDFA Collection
GOOGLE_ANALYTICS_IDFA_COLLECTION_ENABLED = NO

