// Dev.xcconfig.template
// Development environment configuration template
//
// SETUP INSTRUCTIONS:
// 1. Copy this file to: cstudio/Dev.xcconfig (without .template extension)
// 2. Fill in your actual credentials below
// 3. NEVER commit Dev.xcconfig to git (it's in .gitignore)

// Bundle Identifier
PRODUCT_BUNDLE_IDENTIFIER = social.curo.cstudio.dev

// Swift Compilation Conditions (includes DEBUG for debug builds)
SWIFT_ACTIVE_COMPILATION_CONDITIONS = DEBUG DEV

// PostHog Configuration
POSTHOG_API_KEY = YOUR_POSTHOG_API_KEY_HERE
POSTHOG_HOST = https:/$()/us.i.posthog.com

// Supabase Configuration
SUPABASE_URL = YOUR_DEV_SUPABASE_URL_HERE
SUPABASE_ANON_KEY = YOUR_DEV_SUPABASE_ANON_KEY_HERE

// Observability Configuration
// Dev: Analytics disabled to avoid polluting analytics with test data
ANALYTICS_ENABLED = NO
FIREBASE_ENABLED = NO
REMOTE_CONFIG_FETCH_INTERVAL_SECONDS = 3600
ANALYTICS_REQUIRES_EXPLICIT_CONSENT = NO

// Display Name (shows in springboard)
PRODUCT_NAME = cstudio Dev

// App Icon
ASSETCATALOG_COMPILER_APPICON_NAME = AppIcon-Dev

// Optimization Level
SWIFT_OPTIMIZATION_LEVEL = -Onone
GCC_OPTIMIZATION_LEVEL = 0

// Firebase Analytics IDFA Collection
GOOGLE_ANALYTICS_IDFA_COLLECTION_ENABLED = NO
